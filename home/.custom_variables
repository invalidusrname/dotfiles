# make terminal pretty
export TERM=xterm-256color
export CLICOLOR=1
export LSCOLORS=gxgxcxdxbxegedabagacad  # cyan directories
PS1='\u@\h:\w'
PS2="> "

# git
PS1="$PS1\$(parse_git_branch)\$ "

# BitchX
export IRCUSER='invalidusrname'
