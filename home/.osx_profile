# common ENV
export EDITOR=vim
export LC_CTYPE=en_US.UTF-8
export VIM_APP_DIR='/Applications'

# aliases
alias dp="cd ~/Desktop"

# textmate
alias e="echo 'STOP USING THIS KEY DUMBASS'"
alias ubundles="AA=$(pwd); cd /Library/Application\ Support/TextMate; svn up; cd $AA;"

# mysql
alias mysqlstart='sudo //usr/local/bin/mysqld_safe &'
alias mysqlstop='//usr/local/bin/mysqladmin -u root -p shutdown'

### Added by the Heroku Toolbelt
export PATH="/usr/local/heroku/bin:$PATH"
